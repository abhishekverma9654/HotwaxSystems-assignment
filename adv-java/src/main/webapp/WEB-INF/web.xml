<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  
	<servlet>
		<servlet-name>hello1</servlet-name>
		<servlet-class>hello</servlet-class>
	</servlet>
  	<servlet-mapping>
  		<servlet-name>hello1</servlet-name>
  		<url-pattern>/hello</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
		<servlet-name>regfrom</servlet-name>
		<servlet-class>dataModel.Regestration</servlet-class>
	</servlet>
  	<servlet-mapping>
  		<servlet-name>regfrom</servlet-name>
  		<url-pattern>/register</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
		<servlet-name>upfrom</servlet-name>
		<servlet-class>dataModel.Update</servlet-class>
	</servlet>
  	<servlet-mapping>
  		<servlet-name>upfrom</servlet-name>
  		<url-pattern>/update</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
		<servlet-name>deleteac</servlet-name>
		<servlet-class>dataModel.Delete</servlet-class>
	</servlet>
  	<servlet-mapping>
  		<servlet-name>deleteac</servlet-name>
  		<url-pattern>/delete</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
		<servlet-name>logfrom</servlet-name>
		<servlet-class>dataModel.Login</servlet-class>
	</servlet>
  	<servlet-mapping>
  		<servlet-name>logfrom</servlet-name>
  		<url-pattern>/login</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
		<servlet-name>lout</servlet-name>
		<servlet-class>dataModel.Logout</servlet-class>
	</servlet>
  	<servlet-mapping>
  		<servlet-name>lout</servlet-name>
  		<url-pattern>/logout</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
		<servlet-name>searchservlet</servlet-name>
		<servlet-class>dataModel.Search</servlet-class>
	</servlet>
  	<servlet-mapping>
  		<servlet-name>searchservlet</servlet-name>
  		<url-pattern>/search</url-pattern>
  	</servlet-mapping>
	
	<filter>
		<filter-name>loginfilter</filter-name>
		<filter-class>filters.loginFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>loginfilter</filter-name>
		<url-pattern>/dashboard.jsp</url-pattern>
		<url-pattern>/userupdate.jsp</url-pattern>
	</filter-mapping>

	<session-config>
   		<session-timeout>15</session-timeout>
	</session-config>
	
	
</web-app>

